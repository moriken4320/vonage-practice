$(document).ready(function() {
    $("#jquery-div > button").on("click", function () {
        let currentNum = parseInt($("#jquery-div > p:nth-child(2)").text(), 10);
        currentNum++;
        $("#jquery-div > p:nth-child(2)").text(currentNum);
    });
  });

const app = new Vue({
    el: "#vue-div",
    data: {
        counter: 0
    },
    methods: {
        countUp: function() {
            this.counter++;
        }
    },
});